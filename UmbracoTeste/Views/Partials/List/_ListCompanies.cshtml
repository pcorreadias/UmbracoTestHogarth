@model IEnumerable<UmbracoTeste.Models.Companies>


<h2>List Companies</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Sector)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Year_Foundation)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.Sector)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Year_Foundation)
            </td>
        </tr>
    }

</table>
